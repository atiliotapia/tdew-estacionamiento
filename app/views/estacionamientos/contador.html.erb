<h1>Ingresos y Salidas de Autos</h1>
<p>
  <div class="input">
    Seleccionar Local: <select class="large" name="mediumSelect" id="mediumSelect">
      <% @estacionamientos.each do |estacionamiento|%>
      <option value="<%= estacionamiento.id %>"><%= estacionamiento.local %></option>
      <% end %>
    </select>
  </div>
</p>

<div data-role="navbar">
	<ul>
		<li><%= link_to "Ingreso", estacionamientos_path, "data-icon" => "forward" %></li>
		<li><%= link_to "Salida", estacionamientos_path, "data-icon" => "back" %></li>
	</ul>
</div><!-- /navbar -->